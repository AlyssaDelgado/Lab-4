#include <iostream>  

using namespace std;

int main ()

{
  
  float average;
  cout<<"Input your average:"<<endl;
  cin>>average;

  if(average>100)
  cout<<"Invalid data"<<endl;
  else if(average>=90)
  cout<<"A Category"<<endl;
  else if(average>=80)
  cout<<"B Category"<<endl;
  else if(average>=60)
  cout<<"You Pass"<<endl;
  else if(average>=0)
  cout<<"You Fail"<<endl;

return 0;


}


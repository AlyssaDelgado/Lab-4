#include <iostream>
using namespace std;
  
int main() {

cout << "--------------------------------" << endl;
cout<<"Alyssa Delgado"<<endl;
cout<< "Computer Science 1"<<endl;
cout<<"Lab 4"<<endl;
cout<<"February 17th, 2023"<<endl;
cout << "--------------------------------" << endl;

    float fahren, celsius; 
    
    //Instruct user to input temperature 
    cout << "Enter the temperature in fahrenheit\n"; 
    cin >> fahren;
     
    //Calculate the conversion
    celsius = 5 * (fahren - 32) / 9;  
    
    //Output data 
    cout << fahren <<" Fahrenheit is equal to "  
        << celsius <<" Celsius";
      
    return 0;
}
